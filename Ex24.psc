algoritmo "helloworld"
  var dinheiro, dolar, marco, libra:  real
 inicio
   escreval ("Insira salario e vendas")
leia(dinheiro)
dolar <- 1.80
marco <- 2
libra <- 3.57
escreva("real dolar :  ", dinheiro/dolar)
escreva("real marco:  ", dinheiro/marco)
escreva("real libra :  ", dinheiro/libra)

  fimalgoritmo